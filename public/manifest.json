{
  "manifest_version": 3,
  "name": "BookmarkIQ",
  "description": "BookmarkIQ uses intelligent, on-device search to help you instantly find what you’ve saved—fast, private, and effortless.",
  "version": "1.2.0",
  "permissions": [
    "bookmarks"
  ],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "minimum_chrome_version": "92",
  "action": {
    "default_icon": {
      "16": "icons/icon2.png",
      "24": "icons/icon2.png",
      "32": "icons/icon2.png"
    },
    "default_title": "BookmarkIQ",
    "default_popup": "popup.html"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self' 'wasm-unsafe-eval'"
  },
  "icons": {
    "16": "icons/icon2.png",
    "48": "icons/icon2.png",
    "128": "icons/icon2.png"
  }
}