{
  "manifest_version": 3,
  "name": "Find That Bookmark",
  "description": "Search your bookmarks by meaning, not just keywords. Find what you savedâ€”even if you forgot the exact words.",
  "version": "0.0.1",
  "permissions": [
    "bookmarks"
  ],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "minimum_chrome_version": "92",
  "action": {
    "default_icon": {
      "16": "icons/icon2.png",
      "24": "icons/icon2.png",
      "32": "icons/icon2.png"
    },
    "default_title": "Bookmark Search",
    "default_popup": "popup.html"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self' 'wasm-unsafe-eval'"
  },
  "icons": {
    "16": "icons/icon2.png",
    "48": "icons/icon2.png",
    "128": "icons/icon2.png"
  }
}